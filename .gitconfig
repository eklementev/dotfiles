[user]
  name = Eugeny Klementev
  email = eugeny.klementev@gmail.com
[core]
  autocrlf = false
  safecrlf = true
  editor = vim
[push]
  default = simple
[alias]
  st = status
  co = checkout
  ci = commit
  br = branch
  hist = log --pretty=oneline --graph --decorate=full --all --date=short --abbrev-commit
  di = diff
  aa = add --all
  fp = fetch -p -t
  hr = reset --hard HEAD
[merge]
  defaultToUpstream = true

